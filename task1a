class Main {
    public static void main(String[] args) {
            for(int i=0;i<=args.length-1;i=i+2) {
                System.out.print(args[i] + " ");
            }
            System.out.println();
            for (int i=1;i<=args.length-1;i=i+2) {
                for (int j = args[i].length() - 1; j >= 0; j--) {
                    System.out.print(args[i].charAt(j));
                }
                System.out.print(" ");
            }
    }
}
